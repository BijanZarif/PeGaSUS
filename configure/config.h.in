/* Define of package information */
#define PEGASUS_PACKAGE_NAME  "@CMAKE_PROJECT_NAME@"
#define PEGASUS_PACKAGE_URL   "https://github.com/PhilipZwanenburg/PeGaSUS"
#define PEGASUS_VERSION       "@PEGASUS_VERSION@"
#define PEGASUS_VERSION_MAJOR @PEGASUS_VERSION_MAJOR@
#define PEGASUS_VERSION_MINOR @PEGASUS_VERSION_MAJOR@

/* Configured information */
#define PEGASUS_CONFIGURATION_CXX      "@CMAKE_CXX_COMPILER@"
#define PEGASUS_CONFIGURATION_CXXFLAGS "@CMAKE_CXX_FLAGS@"
#define PEGASUS_CONFIGURATION_DATE     "@PEGASUS_CONFIGURATION_DATE@"
#define PEGASUS_CONFIGURATION_HOST     "@PEGASUS_CONFIGURATION_HOST@"
#define PEGASUS_CONFIGURATION_USER     "@PEGASUS_CONFIGURATION_USER@"
#define HOST_SYSTEM "@CMAKE_HOST_SYSTEM_NAME@"

/* Define PEGASUS has access to the library. */
#cmakedefine PEGASUS_HAS_INTEL_MKL           1
#cmakedefine PEGASUS_HAS_GOOGLE_PERF_MINIMAL 1
#cmakedefine PEGASUS_HAS_GOOGLE_TEST         1

/* Set PEGASUS assertions behavior */
#cmakedefine PEGASUS_ASSERTIONS_ABORT   1
#cmakedefine PEGASUS_ASSERTIONS_ASSERT  1
#cmakedefine PEGASUS_ASSERTIONS_DISABLE 1
#cmakedefine PEGASUS_ASSERTIONS_THROW   1
